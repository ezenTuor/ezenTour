function commaNum(num) {
    var len, point, str;

    num = num + "";
    point = num.length % 3
    len = num.length;

    str = num.substring(0, point);
    while (point < len) {
        if (str != "") str += ",";
        str += num.substring(point, point + 3);
        point += 3;
    }
    return str;
}

//영문명 정규식
function fnPressEng(event, obj){
	var val = obj.value;
	if(!fnIsIE()){
		if(event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 37 || event.keyCode == 39 || event.keyCode == 46 || event.keyCode == 21) return;
		obj.value = val.replace(/[0-9]|[^\!-z\s]/gi,"").toUpperCase();
	}else{
		//console.log("ie::"+obj.value);
		obj.value = val.toUpperCase();
	}
}

// 전화번호, 핸드폰번호 정규식
function regTelNo(tel){

	var regTel = /^(01[016789]{1}|02|0[3-9]{1}[0-9]{1})?([0-9]{3,4})?([0-9]{4})$/;

	if(!regTel.test(tel)){
		return false;
	}

	return true;
}

/* ********************************************************
